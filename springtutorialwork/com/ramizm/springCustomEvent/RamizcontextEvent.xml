<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="helloWorld" class="com.ramizm.springCustomEvent.NewsPaperDO">
<property name="newsPaperId" value="Hello World!"/>
</bean>
<bean id="cStartEventHandler"
class="com.ramizm.springCustomEvent.CustomEventPublisher"/>
<bean id="cStopEventHandler"
class="com.ramizm.springCustomEvent.CustomEventListener"/>

</beans>